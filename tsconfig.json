{
    "compilerOptions": {
      "target": "es2020",
      "lib": [
        "es2020",
      ],
      "typeRoots": [
        "./node_modules/@types",
      ],
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "module": "commonjs",
      "pretty": true,
      "sourceMap": true,
      "outDir": "./dist",
      "noEmit": false,
      "esModuleInterop": true,
      "baseUrl": ".",
    },
    "exclude": [
      "node_modules",
    ]
}
